{
  "name": "@liamcottle/rustplus.js",
  "version": "2.4.0",
  "description": "Unofficial NodeJS library for controlling Smart Switches in the PC game Rust",
  "main": "rustplus.js",
  "types": "./typings/index.d.ts",
  "bin": {
    "rustplus": "./cli/index.js"
  },
  "scripts": {
    "build:types": "cp rustplus.proto proto.proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=forceLong=long --ts_proto_out=./typings/ proto.proto ; npm run build:types:compile",
    "build:types:compile": "./node_modules/.bin/tsc --declaration ./typings/proto.ts ; npm run build:types:clean`",
    "build:types:clean": "rm typings/proto.ts ; rm proto.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcottle/rustplus.js.git"
  },
  "keywords": [
    "rust",
    "rust+",
    "rustplus",
    "rustplus-api",
    "companion",
    "api",
    "websocket",
    "smart switch",
    "smart alarm",
    "team chat"
  ],
  "author": "Liam Cottle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcottle/rustplus.js/issues"
  },
  "homepage": "https://github.com/liamcottle/rustplus.js#readme",
  "dependencies": {
    "axios": "^1.2.2",
    "chrome-launcher": "^0.15.0",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "express": "^4.17.1",
    "jimp": "^0.22.7",
    "long": "^5.2.1",
    "protobufjs": "^7.2.1",
    "push-receiver": "^2.1.0",
    "ts-proto": "^1.148.1",
    "type-fest": "^3.7.2",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "ws": "^8.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.6"
  }
}
